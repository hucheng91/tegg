{
  "name": "tegg",
  "version": "0.1.3",
  "description": "eggjs with typescript",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "lint:fix": "eslint . --ext .ts --fix",
