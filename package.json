    "url": "https://github.com/eggjs/tegg/issues"
  },
  "homepage": "https://github.com/eggjs/tegg#readme",
  "dependencies": {
    "@artus/core": "^1.0.0-beta.9",
    "@koa/router": "^10.1.1",
    "detect-port": "^1.3.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/node": "^18.0.0",
    "axios": "^0.27.2",
    "eslint": "^8.17.0",
    "eslint-config-egg": "^12.0.0",
    "ioredis": "^5.1.0",
    "jest": "^28.1.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.3"
  }
